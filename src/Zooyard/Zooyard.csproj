<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net6.0</TargetFrameworks>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>
	  <NoWarn>$(NoWarn);1591</NoWarn>
	  <PackageLicenseUrl>https://github.com/wutao0315/zooland/blob/master/LICENSE</PackageLicenseUrl>
	  <Copyright>MIT</Copyright>
	  <PackageProjectUrl>https://github.com/wutao0315/zooland</PackageProjectUrl>
	  <PackageReleaseNotes />
	  <Description>Zooyard RPC Framework microservice</Description>
	  <Authors>wutao0315</Authors>
	  <Version>1.2.1</Version>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Config\**" />
    <Compile Remove="Loader\**" />
    <EmbeddedResource Remove="Config\**" />
    <EmbeddedResource Remove="Loader\**" />
    <None Remove="Config\**" />
    <None Remove="Loader\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="DynamicProxy\IInterceptor.cs" />
    <Compile Remove="DynamicProxy\InterfaceProxy.cs" />
    <Compile Remove="Initialize.cs" />
    <Compile Remove="IRegistry.cs" />
    <Compile Remove="IRegistryHealthChecks.cs" />
    <Compile Remove="IRegistryHost.cs" />
    <Compile Remove="IRegistryKeyValues.cs" />
    <Compile Remove="IRegistryService.cs" />
    <Compile Remove="IRegistryServiceFind.cs" />
    <Compile Remove="IServer.cs" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.ClearScript.V8" Version="7.3.1" />
    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="6.0.0" />
    <PackageReference Include="System.Reflection.Emit" Version="4.7.0" />
    <PackageReference Include="System.Runtime.Caching" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="6.0.0" />
  </ItemGroup>


  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>$(AssemblyName).Tests</_Parameter1>
      <!-- We use the value of AssemblyName to declare the value of the attribute -->
    </AssemblyAttribute>
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.Rpc</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.Extensions</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.Extensions</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.GrpcImpl</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.HttpImpl</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.Rpc.NettyImpl</_Parameter1>
    </AssemblyAttribute>
	  <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
		  <_Parameter1>Zooyard.DotNettyImpl</_Parameter1>
	  </AssemblyAttribute>
	  <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>Zooyard.ThriftImpl</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
